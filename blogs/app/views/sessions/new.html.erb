<h1>Sessions</h1>

<%= form_tag sessions_path do %>
	<div class="field">
		<%= label_tag :email, params[:email] %> <br />
		<%= text_field_tag :email, params[:email] %>
	</div>
	<div class="field">
		<%= label_tag :password, params[:password] %> <br />
		<%= password_field_tag :password, params[:password] %>
	</div>
	<div class="action">
		<%= submit_tag "Log In" %>
	</div>
<% end %>
